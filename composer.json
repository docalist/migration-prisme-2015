{
    "name": "docalist/migration-prisme-2015",
    "description": "Docalist : Migration Prisme 2015",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Daniel Ménard",
            "email": "daniel.menard@laposte.net"
        }
    ],
    "require": {
        "php": ">=7.2",
        "composer/installers": "^1.6"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload" : {
        "psr-4" : {
            "MigrationPrisme2015\\" : "src"
        }
    },
    "config" : {
        "sort-packages" : true,
        "optimize-autoloader" : true,
        "classmap-authoritative" : true
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0-dev"
        }
    }
}
